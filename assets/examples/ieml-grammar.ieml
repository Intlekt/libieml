@inflection en"reflexive" 				fr"réflexif"	VERB		'E:.-wu.-t.o.-\''		.

@inflection en"participle" en"quasi adjective" 	fr"participe" fr"quasi adjectif" VERB	'E:.-\'we.-T:.-\'t.o.-\',' 	.

// NOUNS ~INFLECTIONS
@inflection en"singular" 				fr"singulier"	NOUN		'E:.wo.-'			.
@inflection en"two" 					fr"deux"		NOUN	'E:.wa.-'			.
@inflection en"three" 				fr"trois"		NOUN	'E:.wu.-'			.
@inflection en"plural" 				fr"pluriel"		NOUN	'E:.we.-'			.

@inflection en"all"					fr"tous"		NOUN	'E:A:.-t.u.-\''		.

@inflection en"non contiguous" 			fr"non contigu"	NOUN		'E:.-\',b.-S:.U:.-\'y.-\'A:.-\',_'.

@inflection en"noun" VERB 'S:'.
@inflection en"verb" VERB 'B:'.

