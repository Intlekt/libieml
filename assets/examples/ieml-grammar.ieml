// MATHEMATICAL RELATIONS

@node en"reflexivity relation" fr"relation réflexive"
(
0 #is reflexive,				
1 #relation	
).

@node en"irreflexivity relation" fr"relation irréflexive"
(
0 ~not #is reflexive,				
1 #relation	
).

@node en"symmetric relation" fr"relation symétrique"
(
0 #is symmetric of,			
1 #relation	
).				

@node en"asymmetric relation" fr"relation asymétrique" 
(
0 ~not #is symmetric of,				
1 #relation 	
).				

@node en"transitive relation" fr"relation transitive"
(
0 #is transitive,					
1 #relation
).					

@node en"atransitive relation" fr"relation atransitive"
(
0 ~not #is transitive,					
1 #relation 
).

@node en"equivalence relation" fr"relation d équivalence"
(
0 ~reciprocal #combine,
1 &and [#symmetric relation #transitive relation #reflexivity relation]
).

@node en"proximity relation" fr"relation de proximité"
(
0 ~reciprocal #combine,
1 &and [#symmetric relation #atransitive relation #reflexivity relation]
).

@node en"complementary relation" fr"relation complémentaire"
(
0 ~reciprocal #combine,
1 &and [#asymmetric relation #atransitive relation #irreflexivity relation]
).

@node en"order relation" fr"relation d ordre"
(
0 ~reciprocal #combine,
1 &and [#asymmetric relation #transitive relation #irreflexivity relation]
).

@node en"logical implication" en"logically implies" fr"implication logique" fr"implique logiquement"						
(
0 #inference,
4 *by #conditional determination
).