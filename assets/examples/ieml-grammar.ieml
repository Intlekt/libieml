// *AUXILIARIES ROLE7 intentions
@auxiliary en"in context" 				fr"en contexte"		7 'E:.-\'h.o.d.-U:.-\'x.-\','	.
@auxiliary en"variable variable" 			fr"variable variable" 7	'E:.-b.o.-\'E:.-b.o.-\','	.

// *AUXILIARY ROLE8 manner
@auxiliary 	en"same" 				fr"même"			8 'E:.A:.g.-'	.
@auxiliary	en"name of" 				fr"nom de" 		8	'E:.A:.e.-'	.
@auxiliary 	en"moral feature possessed by" 		fr"trait moral possédé par"	 8 'E:.A:.o.-'	. 	

