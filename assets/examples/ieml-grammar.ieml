//~INFLECTIONS

@language en.

// VERBS ~INFLECTIONS
@inflection en"if" 					fr"si" 		VERB		   'E:S:A:.'			.	
@inflection en"then" 				fr"alors"	VERB    		'E:T:U:.'			.	
@inflection en"not" 					fr"non"		VERB		'E:U:A:.'			.	

@inflection en"passive" 				fr"passif" 		VERB	'E:.-wo.-t.o.-\''	. 
@inflection en"active" 				fr"actif"		VERB	'E:.-wa.-t.o.-\''		.
