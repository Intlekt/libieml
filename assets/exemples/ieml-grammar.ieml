// ¶¶ PHRASES

// ¶ IEML BASICS 

@node en"' fr"'
(
0 #language,
8 *moral property possessed by #collective intelligence
).

@node en"IEML grammar" fr"grammaire d'' 		
(
0 #grammar,
4 *part of #IEML
).

@node en"USL" fr"USL" 	 			
(
0 #expression,
4 &and
[
*formal cause #IEML grammar
*part of #IEML
]
).

@node "IEML phrase" fr"phrase"
(
0 phrase,
4 *part of #IEML	
).

@node en"phrase role" fr"rôle de phrase"
(
0 #role,
4 *part of #IEML phrase
).

¶ IEML DICTIONARY

@node en"IEML dictionary" fr"dictionnaire d''
(
0 ~all #word,
4 *part of #IEML
).

@node en"IEML primitives" fr"primitives ' 	
(
0 ~plural #first principles,
4 *part of #IEML dictionary
).

@node en"IEML primitive" fr"primitive ' 	
(
0 ~singular first principles,
4 *part of #IEML dictionary
).	

@node en"IEML element" fr"élément d''	
(
0 ~singular #atom,
4 *part of #IEML dictionary
).

@node en"seme" fr"sème"					
(
0 #one third,	
4 *part of #IEML element			
).

@node en"IEML radical" fr"radical '
(
0 #glue,
1 ! ~singular #word,
2 &or inclusive [~two #atom ~three #atom],
4 *part of #IEML dictionary
).

@node fr"IEML element paradigm" fr"paradigme d'éléments" 	
(
0 #contain,
1 ! #paradigm,
2 ~plural #IEML element		
).

@node en"IEML radical paradigm" fr"paradigme de radicaux"
0 #contain,
1 ! #paradigm,
2 ~plural #IEML radical		
).

@node en"binary dialectic"		fr"dialectique binaire"		
(
0 #dialectic
8 #binary
).

@node en"ternary dialectic"		fr"dialectique ternaire"		
(
0 #dialectic
8 #ternary
).

// + autres dialectiques....

// ¶ #SYNTACTIC CLASSES

@node en"IEML category" fr"catégorie '
(
0 	#express,
1 ! 	#USL,
2 	#discursive content
).



// ¶¶¶¶¶¶ @node en"phrase category" fr"catégorie phrase" = phrase IEML 
// ¶¶ CRÉER JUSTE UNE RELATION AU LIEU D'UN MOT
(
0 	#express			,
1 ! 	#phrase IEML			,
2 	#discursive content
)					.

// ¶¶¶¶¶¶ par opposition aux particules grammaticales 
@node en"elementary category"  fr"catégorie élémentaire"			
(
0 #express				,
1 ! #IEML element			,
2 #discursive content
)					.

@node en"IEML auxiliary" fr"auxiliaire '	
(
0 #precise,
1 ! #IEML element,
2 #phrase role
).

@node en"IEML junction" fr"jonction '
(
0 #glue,
1 ! #IEML element,
2 ~plural #IEML category
).

@node en"IEML inflection" fr"flexion"
(
0 express,
1 ! #IEML element
2 #context variation,
8 *with #IEML category		
).

@node en"tool word" fr"mot outil""particule grammaticale"
(
&inclusive or [#IEML inflection #IEMLjunction #IEMLauxiliary]
)

